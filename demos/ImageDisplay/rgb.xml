<?xml version="1.0" ?>

<feelin:application>
	<Application>
		<Window id="win" Title="RGB color values" Open="true">
			<Group SameSize="true">
				<Text Contents="red" HCenter="true" style="background: #F00" />
				<Text Contents="red" HCenter="true" style="background: #FF0000" />
				<Text Contents="red" HCenter="true" style="background: rgb(255,0,0)" />
				<Text Contents="red" HCenter="true" style="background: rgb(100%, 0%, 0%)" />
			</Group>
		</Window>
   	</Application>

   	<notify>
		<win attribute="CloseRequest" value="true" target="application" method="Shutdown" />
   	</notify>
</feelin:application>
