<?xml version="1.0" ?>

<!ENTITY hello "Hello" >

<feelin:application>
	<Application>
		<Window id="win" Title="Feelin : &hello;" Open="true">
			<Button>_&hello;</Button>
		</Window>
   	</Application>

   	<notify>
		<win attribute="CloseRequest" value="true" target="application" method="Shutdown" />
   	</notify>
</feelin:application>
