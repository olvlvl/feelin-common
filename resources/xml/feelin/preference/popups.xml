<?xml version="1.0" ?>

<!ENTITY locale.popbutton "PopButton" >
<!ENTITY locale.popstring "PopString" >
<!ENTITY locale.images "Images" >
<!ENTITY locale.frame "Frame" >
<!ENTITY locale.back "Back" >
<!ENTITY locale.scheme "Scheme">
<!ENTITY locale.font "Font" >
<!ENTITY locale.file "File" >
<!ENTITY locale.preparse "PreParse" >

<feelin:objects>
   <Group>
      <HGroup>
         <Group class="box" Rows="2" Title="&locale.popbutton;" style="spacing-vertical: 2;">
            <PopFrame id="popbutton:frame" Couple="true"/>
            <PopImage id="popbutton:back" Couple="true"/>
            <PopScheme id="popbutton:scheme" />
            <VLabel>&locale.frame;</VLabel>
            <VLabel>&locale.back;</VLabel>
            <VLabel>&locale.scheme;</VLabel>
         </Group>

         <Group class="box" Rows="2" Title="&locale.popstring;" style="spacing-vertical: 2;">
            <PopFrame id="popstring:frame" Couple="true"/>
            <PopImage id="popstring:back" Couple="true"/>
            <PopScheme id="popstring:scheme" />
            <VLabel>&locale.frame;</VLabel>
            <VLabel>&locale.back;</VLabel>
            <VLabel>&locale.scheme;</VLabel>
         </Group>
      </HGroup>
                
      <Group class="box" Rows="2" Title="&locale.images;" style="spacing-vertical: 2;">
         <PopImage id="popfont:image" Couple="true"/>
         <PopImage id="popfile:image" Couple="true"/>
         <PopImage id="poppreparse:image" Couple="true"/>
         <VLabel>&locale.font;</VLabel>
         <VLabel>&locale.file;</VLabel>
         <VLabel>&locale.preparse;</VLabel>
      </Group>
   </Group>
</feelin:objects>
