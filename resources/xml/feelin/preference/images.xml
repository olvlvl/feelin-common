<?xml version="1.0" ?>

<feelin:objects>
   <Group class="box" Rows="2" Title="Checkbox" style="spacing-vertical: 2;">
      <PopImage id="checkbox:back" Couple="true" />
      <PopFrame id="checkbox:frame" Couple="true" />
      <PopScheme id="checkbox:color-scheme" />
      <PopImage id="checkbox:image" Couple="true" />
      <Checkbox>false</Checkbox>
      <VLabel>Back</VLabel>
      <VLabel>Frame</VLabel>
      <VLabel>Scheme</VLabel>
      <VLabel>Image</VLabel>
      <VLabel>Example</VLabel>
   </Group>

   <Group class="box" Rows="2" Title="Radio" style="spacing-vertical: 2;">
      <PopImage id="radio-button:back" />
      <PopScheme id="radio-button:color-scheme" />
      <PopImage id="radio-button:image" Couple="true" />
      <Image class="radio-button" InputMode="toggle" Selected="false" SetMax="both" />
      <VLabel>Back</VLabel>
      <VLabel>Scheme</VLabel>
      <VLabel>Image</VLabel>
      <VLabel>Example</VLabel>
   </Group>
</feelin:objects>
