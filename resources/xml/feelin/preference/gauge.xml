<?xml version="1.0" ?>
   
<feelin:objects>
    <Group class="box" Rows="2" Title="Aspect" SameSize="true" style="spacing-vertical: 2;">
        <PopFrame id="gauge:frame" />
        <PopScheme id="gauge:color-scheme" />
        <VLabel>Frame</VLabel>
        <VLabel>Scheme</VLabel>
    </Group>
      
    <HGroup>
        <Group class="box" Rows="2" Title="Back" SameSize="true" style="spacing-vertical: 2;">
            <PopImage id="gauge:back-horizontal" />
            <PopImage id="gauge:back" />
            <VLabel>Horizontal</VLabel>
            <VLabel>Vertical</VLabel>
        </Group>

        <Group class="box" Rows="2" Title="Image" SameSize="true" style="spacing-vertical: 2;">
            <PopImage id="gauge:image-horizontal" />
            <PopImage id="gauge:image" />
            <VLabel>Horizontal</VLabel>
            <VLabel>Vertical</VLabel>
        </Group>
    </HGroup>

    <Group class="box" Title="Example">
        <Gauge id="example" Horizontal="true" SetMax="height" Max="100" />
    </Group>
</feelin:objects>
